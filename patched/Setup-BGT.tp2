    WRITE_LONG 0x2a4 "%known_num%"
    WRITE_LONG 0x2a8 "%meminfo_off%"
    WRITE_LONG 0x2b0 "%mem_off%"
    WRITE_LONG 0x2b8 "%slot_off%"
