      PATCH_IF ("SPPR313" STRING_COMPARE_CASE "%spell%" = 0) BEGIN
        WRITE_ASCII ("%mem_off%" + (("%index%" - 1) * 0x0C)) ~sppr314~ #8
      END
    END
